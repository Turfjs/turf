    "@turf/boolean-point-on-line": "^7.0.0-alpha.1",
    "@turf/helpers": "^7.0.0-alpha.1",
    "@turf/invariant": "^7.0.0-alpha.1",
    "@turf/intersection-points": "^7.0.0-alpha.1",
    "geojson-polygon-self-intersections": "1.2.x",
    "tslib": "^2.3.0"
  }

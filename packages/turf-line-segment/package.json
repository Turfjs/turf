{
  "name": "@turf/line-segment",
  "version": "6.2.0-alpha.2",
  "description": "turf line-segment module",
  "author": "Turf Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turfjs/turf/issues"
  },
  "homepage": "https://github.com/Turfjs/turf",
  "repository": {
    "type": "git",
    "url": "git://github.com/Turfjs/turf.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "turf",
    "line",
    "segment"
  ],
  "main": "dist/js/index.js",
  "module": "dist/es/index.js",
  "exports": {
    "import": "./dist/es/index.js",
    "require": "./dist/js/index.js"
  },
  "types": "dist/js/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "bench": "npm-run-all prepare bench:run",
    "bench:run": "node bench.js",
    "docs": "node ../../scripts/generate-readmes",
    "prepare": "npm-run-all prepare:*",
    "prepare:es": "tsc --outDir dist/es --module esnext --declaration false && echo '{\"type\":\"module\"}' > dist/es/package.json",
    "prepare:js": "tsc",
    "test": "npm-run-all prepare test:*",
    "test:tape": "node -r esm test.js"
  },
  "devDependencies": {
    "@types/tape": "*",
    "benchmark": "*",
    "load-json-file": "*",
    "npm-run-all": "*",
    "tape": "*",
    "tslint": "*",
    "typescript": "*",
    "write-json-file": "*"
  },
  "dependencies": {
    "@turf/helpers": "^6.2.0-alpha.2",
    "@turf/invariant": "^6.2.0-alpha.2",
    "@turf/meta": "^6.2.0-alpha.2"
  }
}

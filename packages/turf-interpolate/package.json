{
  "name": "@turf/interpolate",
  "version": "7.0.0-alpha.2",
  "description": "turf interpolate module",
  "author": "Turf Authors",
  "contributors": [
    "Stefano Borghi <@stebogit>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Turfjs/turf/issues"
  },
  "homepage": "https://github.com/Turfjs/turf",
  "repository": {
    "type": "git",
    "url": "git://github.com/Turfjs/turf.git"
  },
  "funding": "https://opencollective.com/turf",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "turf",
    "idw",
    "interpolate"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "bench": "tsx bench.ts",
    "build": "tsup index.?s --dts --format esm,cjs --cjsInterop --splitting",
    "docs": "tsx ../../scripts/generate-readmes.ts",
    "test": "npm-run-all --npm-path npm test:*",
    "test:tape": "tsx test.ts",
    "test:types": "tsc --esModuleInterop --noEmit --strict types.ts"
  },
  "devDependencies": {
    "@turf/truncate": "workspace:^",
    "benchmark": "^2.1.4",
    "chromatism": "^3.0.0",
    "load-json-file": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "tape": "^5.7.2",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "write-json-file": "^5.0.0"
  },
  "dependencies": {
    "@turf/bbox": "workspace:^",
    "@turf/centroid": "workspace:^",
    "@turf/clone": "workspace:^",
    "@turf/distance": "workspace:^",
    "@turf/helpers": "workspace:^",
    "@turf/hex-grid": "workspace:^",
    "@turf/invariant": "workspace:^",
    "@turf/meta": "workspace:^",
    "@turf/point-grid": "workspace:^",
    "@turf/square-grid": "workspace:^",
    "@turf/triangle-grid": "workspace:^"
  }
}

  <desc>Several points collinear and overlapping</desc>
  <a>
    MULTIPOINT((130 240), (130 240), (130 240), (570 240), (570 240), (570 240), (650 240))
  </a>
<test>
  <op name="convexhull" arg1="A">
    LINESTRING(130 240, 650 240)
  </op>
</test>
</case>

<case>
